<!doctype HTML>
<html>
    <head>
        <link href="main.css" rel="stylesheet" type="text/css">
        <script>
            function sessionData(){
                if (sessionStorage.getItem("customerName") != null) {
                    document.getElementById("result").innerHTML = sessionStorage.getItem("customerName");
                } else {
                    document.getElementById("result").innerHTML = "If you add a passenger on the home page it will show up here";
                }
            }
            function displayImage(){
                var canvas = document.getElementById("myCanvas");
                var ctx = canvas.getContext("2d");
                var img = document.getElementById("testingImg");
                ctx.drawImage(img,0,0);
            }
        </script>
    </head>
    <body onload="sessionData(); getMyVideo();">
        <div class="topnav">
            <a class="active" href="/">Home</a>
            <a href="/page2">Page 2</a>
        </div>
        <div id="result"></div>
        <div class="theater">
            <img id="testingImg" src="data:image/jpeg;base64,">
            <video id="myVideo" src="https://youtu.be/LH7eegimHUQ" controls="false"></video>
        </div>
        <br><br>
        <div class="dropdown">
            <button class="dropbtn">Canvas Options</button>
            <div class="dropdown-content">
                <button onclick="displayImage()">Display Image</button>
                <button onclick="something()">Something</button>
                <button onclick="something2()">Something2</button>
            </div>
        </div>
        <div class="boxCanvas">
            <canvas id="myCanvas" width="200" height="100"></canvas>
        </div>
    </body>
</html>